<h1>All Products<h1>

<br>
<br>

<table>
	<tr>
		<th>Name</th>
		<th>Brand</th>
		<th>Price</th>
		<th>Size</th>
		<th>Category</th>
	</tr>
	
	<% @products.each do |product| %>
		<tr>
			<td><%= product.name %></td>
			<td><%= product.brand %></td>
			<td><%= product.price %></td>
			<td><%= product.size %></td>
			<td><%= product.category %></td>
		</tr>
	<% end %>

</table>