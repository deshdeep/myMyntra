<h1>Log in</h1>
	<%= form_for(:session, url: login_path) do |f|%>
		<p>
			<%= f.label :email%>&nbsp;
			<%= f.email_field :email%>
		</p>
		
		<p>
			<%= f.label :password%>&nbsp;
			<%= f.password_field :password%>
		</p>
		
		<p>
			<%= f.submit "Log in"%>
		</p>
	<%end%>
	
		<p>New User? <%= link_to "SignUp Now", new_customer_path%></p>